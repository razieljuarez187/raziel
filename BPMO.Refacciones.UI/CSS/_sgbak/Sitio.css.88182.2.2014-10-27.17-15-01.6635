/*--------------------------------Clases generales del sitio-----------------------------*/
.trDerecha
{
    text-align: right;
}
.trAlinearDerecha tr td:first-child
{
    text-align: right;
}
#TablaBusqueda
{
    text-transform: uppercase;
}
#TablaBusqueda tr
{
    height: 30px;
}
.TipoLetra
{
    font-family: Century Gothic, Arial, Verdana, Serif;
}
/*--------Estilos para las notas de taller-------------*/
.PanelNotasTaller
{
    min-height: 50px;
    max-height: 400px;
    background-color: transparent;
    margin: 20px 0 0 17px;
}
.SinNotas
{
    float: left;
    margin: 50px auto;
    border-width: 1px;
    border-style: solid;
    font-size: 18px;
    margin: auto;
}
.EncabezadoNotaTaller
{
    clear: both;
    padding: 0;
    text-align: center;
    width: 100px;
    border-right: 1px solid orange;
    font-size: 12px;
    font-weight: bold;
}
/*-------------------Valorizado---------------------*/
.tdValorizado td
{
    text-align: right;
}
.textValorizado
{
    background: #E6E6E6;
    border: 1px solid #CCCCCC;
    padding: 1px;
    text-align: right;
    border-radius: 5px;
    height: 20px;
    font-size: 14px;
    color: Black;
}
/*------------------Estilos para las opciones de búsqueda---------------------*/
.textBoxSearch
{
    background: #FFFFFF url('../Imagenes/buscar.png') no-repeat 2px 2px;
    padding: 2px 2px 2px 18px;
    border: 1px solid #CCCCCC;
    font-size: 11px;
}
.textBoxDisabledSearch
{
    background: #E6E6E6;
    border: 1px solid #CCCCCC;
    padding: 1px;
    font-size: 11px;
}
.textBoxDisabled
{
    background: #E6E6E6;
    border: 1px solid #CCCCCC;
    padding: 1px;
}
.txtMoneda
{
    text-align: right;
    padding: 2px 4px 2px 4px;
    border: 1px solid #CCCCCC;
    font-family: "Helvetica Neue" , "Lucida Grande" , "Segoe UI" , Arial, Helvetica, Verdana, sans-serif;
}
.monedaDisabled
{
    text-align: right;
    background: #E6E6E6;
    padding: 1px;
    border: 1px solid #CCCCCC;
}
.ColorValidator
{
    color: Red;
}
.imgBtnSelect
{
    background-image: url('../Imagenes/select.png');
    background-repeat: no-repeat;
    width: 17px;
    height: 17px;
}
/*-------------------------Sección para los reportes-----------------*/
.ArbolAdscripcion
{
    font-family: Century Gothic, Arial, Verdana, Serif;
    font-size: 12px;
    min-height: 250px;
    text-transform: uppercase;
    margin-left: 2px;
    width: 98%;
}
.NodosArbolAdscripcion
{
    padding-top: 5px;
    color: black;
    width: 100%;
    height: 20px;
    cursor: default;
}
.NodosArbolAdscripcionRaiz
{
    width: 100%;
    min-width: 185px;
    height: 20px;
    color: black;
