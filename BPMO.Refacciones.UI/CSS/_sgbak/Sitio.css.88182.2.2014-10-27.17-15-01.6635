/*-----------------------------------Estilos para los mensajes de alertas----------------------------------------------------*/
/*--------Alertas Despliegue Barra Superior------------*/
.info, .exito, .alerta, .error
{
    font-family: Arial, Helvetica, sans-serif;
    font-size: 13px;
    border: 1px solid;
    margin: 10px 10px 0px 10px;
    padding: 15px 10px 15px 50px;
    background-repeat: no-repeat;
    background-position: 10px center;
    position: relative;
    width: 94%;
}
.info
{
    color: #00529B;
    background-color: #BDE5F8;
    background-image: url('../imagenes/info.png');
}
.exito
{
    color: #4F8A10;
    background-color: #DFF2BF;
    background-image: url('../imagenes/exito.png');
}
.alerta
{
    color: #9F6000;
    background-color: #FEEFB3;
    background-image: url('../imagenes/alerta.png');
}
.error
{
    color: #D8000C;
    background-color: #FFBABA;
    background-image: url('../imagenes/error.png');
}

/*----------Alertas GrowUI-------------*/
div.growlUIExito
{
    background: url(../imagenes/exito.png) no-repeat 5px 5px;
}
div.growlUIAlerta
{
    background: url(../imagenes/alerta.png) no-repeat 5px 5px;
}
div.growlUIInfo
{
    background: url(../imagenes/info.png) no-repeat 5px 5px;
}
div.growlUI h1, div.growlUI h2
{
    color: white;
    padding: 0px 2px 2px 50px;
    text-align: left;
}

/*----------------Estilos para los GridView------------------*/
.Grid
{
    border: 1px solid #cccccc;
    margin-top: 0px;
}
/*-----------Sección del encabezado------------*/
.GridHeader
{
    background-color: #5c5e5d;
    color: White;
    font-size: 1em;
    font-weight: bold;
    text-transform: uppercase;
}
.GridHeader a
{
    color: White;
}
.GridHeader a:hover
{
    color: Yellow;
    text-decoration: none;
}
.GridHeaderBitacora
{
    background-color: #cccccc;
    color: White;
    font-size: 1em;
    font-weight: bold;
    text-transform: uppercase;
}
.GridHeaderBitacora a
{
    color: White;
}
.GridHeaderBitacora a:hover
{
    color: Yellow;
    text-decoration: none;
}
/*----------Sección de paginación-----------*/
.GridPager
{
    background-color: #5c5e5d;
    color: white;
    font-size: small;
    font-weight: bold;
}
.GridPager a
{
    color: White;
    font-weight: normal;
}
.GridPager a:hover
{
    color: Yellow;
    text-decoration: none;
}

.GridSelectedRow, .GridRow, .GridAlternatingRow, .GridFooter
{
    border-bottom: solid 1px #f2f2f2;
    border-left: solid 1px #f2f2f2;
    border-right: solid 1px #f2f2f2;
}
.GridSelectedRow
{
    background-color: #E2DED6;
    color: #333333;
    font-size: 8pt;
    font-weight: bold;
}
.GridRow
{
    background-color: white;
    color: Black;
}
.GridAlternatingRow
{
    background-color: #f2f2f2;
    color: black;
}
/*------ Sección del pie-----*/
.GridFooter
{
    background-color: #5c5e5d;
    color: black;
    font-size: small;
    font-weight: bold;
}

